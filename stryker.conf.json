{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker/master/packages/api/schema/stryker-core.json",
  "comment": "This config was generated using a preset. Please see the handbook for more information: https://github.com/stryker-mutator/stryker-handbook/blob/master/stryker/guides/react.md#react",
  "mutate": [
    "src/**/*.js?(x)",
    "!src/**/*@(.test|.spec|Spec).js?(x)",
    "!src/serviceWorker.js",
    "!src/react-auth0-spa.js",
    "!src/setupTests.js",
    "!src/**/index.js",
    "!src/tests/**"
  ],
  "mutator": "javascript",
  "testRunner": "jest",
  "reporters": [
    "progress",
    "clear-text",
    "html"
  ],
  "coverageAnalysis": "off",
  "jest": {
    "projectType": "create-react-app"
  },

  "thresholds": {
    "break": 100,
    "high": 100,
    "low": 100
  }
}
